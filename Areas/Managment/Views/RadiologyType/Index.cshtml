@model IEnumerable<Clinc_Care_MVC_Grad_PROJ.Areas.Managment.Models.RadiologyType>


@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.RadiologyName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>

                @Html.DisplayFor(modelItem => item.RadiologyName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.RadiologyTypeId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.RadiologyTypeId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.RadiologyTypeId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
